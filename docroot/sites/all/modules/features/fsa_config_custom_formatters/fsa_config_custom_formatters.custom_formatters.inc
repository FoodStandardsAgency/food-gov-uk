<?php

/**
 * @file
 * Bulk export of custom_formatters objects generated by Bulk export module.
 */

/**
 * Implements hook_custom_formatters_defaults().
 */
function export_custom_formatters_defaults() {
  $formatters = array();

  $formatter = new stdClass();
  $formatter->disabled = FALSE; /* Edit this to true to make a default formatter disabled initially */
  $formatter->api_version = 2;
  $formatter->name = 'clone_of_type_and_link';
  $formatter->label = 'Summary of responses';
  $formatter->description = 'Formatter for the consultation summary of responses';
  $formatter->mode = 'php';
  $formatter->field_types = 'file';
  $formatter->code = '    $output = \'\';
    foreach ($variables[\'#items\'] as $item) {
       $output .= \'<div class="file-item">\';
       $item[\'filename\'] = t(\'A summary of responses is now available\');
       $output .= theme(\'file_link\', array(\'file\' => (object) $item));

       $size = $item[\'filesize\'];
       if ($size > 0) {
         $units = array("B", "KB", "MB", "GB", "TB" );
         $digitGroups = (integer) (log10($size)/log10(1024));
         $output .=  \'<span class="filesize">(\'. number_format(($size/pow(1024, $digitGroups)), 2, \'.\', \'\') . \' \'.  $units[$digitGroups] .\')</span>\';
      }
      $output .= \'</div>\';
    }
    return $output;';
  $formatter->fapi = '';
  $formatters['clone_of_type_and_link'] = $formatter;

  $formatter = new stdClass();
  $formatter->disabled = FALSE; /* Edit this to true to make a default formatter disabled initially */
  $formatter->api_version = 2;
  $formatter->name = 'example_php_image';
  $formatter->label = 'Example: Image (PHP)';
  $formatter->description = 'A PHP example formatter; Display a Thumbnail image linked to a Large image.';
  $formatter->mode = 'php';
  $formatter->field_types = 'image';
  $formatter->code = '// Formatter settings.
$settings = $variables[\'#display\'][\'settings\'];

$element = array();
foreach ($variables[\'#items\'] as $delta => $item) {
  $element[$delta] = array(
    \'#type\' => \'link\',
    \'#href\' => file_create_url(image_style_path($settings[\'destination_image_style\'], $item[\'uri\'])),
    \'#title\' => theme(\'image_style\', array(
      \'style_name\' => $settings[\'source_image_style\'],
      \'path\' => $item[\'uri\'],
    )),
    \'#options\' => array(
      \'html\' => TRUE,
    ),
  );
}
return $element;';
  $formatter->fapi = '$form = array();
$form[\'source_image_style\'] = array(
  \'#title\' => t(\'Source image styles\'),
  \'#multiple_toggle\' => \'1\',
  \'#required\' => \'0\',
  \'#type\' => \'custom_formatters_image_styles\',
  \'#weight\' => \'0\',
);
$form[\'destination_image_style\'] = array(
  \'#title\' => t(\'Destination image styles\'),
  \'#multiple_toggle\' => \'1\',
  \'#required\' => \'0\',
  \'#type\' => \'custom_formatters_image_styles\',
  \'#weight\' => \'1\',
);
';
  $formatters['example_php_image'] = $formatter;

  $formatter = new stdClass();
  $formatter->disabled = FALSE; /* Edit this to true to make a default formatter disabled initially */
  $formatter->api_version = 2;
  $formatter->name = 'example_token_image';
  $formatter->label = 'Example: Image (HTML + Token)';
  $formatter->description = 'A HTML + Tokens example formatter; Display a Thumbnail image linked to a Large image.';
  $formatter->mode = 'token';
  $formatter->field_types = 'image';
  $formatter->code = '<a href="[image-field-value:styles:url:large]"><img src="[image-field-value:styles:url:thumbnail]" alt="[image-field-value:alt]" title="[image-field-value:title]" /></a>';
  $formatter->fapi = '';
  $formatters['example_token_image'] = $formatter;

  $formatter = new stdClass();
  $formatter->disabled = FALSE; /* Edit this to true to make a default formatter disabled initially */
  $formatter->api_version = 2;
  $formatter->name = 'title_type_size';
  $formatter->label = 'File title, type, and size';
  $formatter->description = 'Formatter for Paper downloads.';
  $formatter->mode = 'php';
  $formatter->field_types = 'file';
  $formatter->code = '    $output = \'\';
    foreach ($variables[\'#items\'] as $item) {
       $output .= \'<div class="file-item">\';

       if (isset($item[\'field_file_title\'][LANGUAGE_NONE][0][\'value\'])) {
         $item[\'filename\'] = $item[\'field_file_title\'][LANGUAGE_NONE][0][\'value\'] . \' \';
       }

       if (isset($item[\'field_file_type\']) && isset($item[\'field_file_type\'][LANGUAGE_NONE][0][\'value\'])) {
         $field = field_info_field(\'field_file_type\');
         $key = $item[\'field_file_type\'][LANGUAGE_NONE][0][\'value\'];
         $item[\'filename\'] .= $field[\'settings\'][\'allowed_values\'][$key];
       }

       $output .= theme(\'file_link\', array(\'file\' => (object) $item));

       $size = $item[\'filesize\'];

       if ($size > 0) {
         $units = array("B", "KB", "MB", "GB", "TB" );
         $digitGroups = (integer) (log10($size)/log10(1024));
         $output .=  \'<span class="filesize">(\'. number_format(($size/pow(1024, $digitGroups)), 2, \'.\', \'\') . \' \'.  $units[$digitGroups] .\')</span>\';
      }
      $output .= \'</div>\';
    }
    return $output;';
  $formatter->fapi = '';
  $formatters['title_type_size'] = $formatter;

  $formatter = new stdClass();
  $formatter->disabled = FALSE; /* Edit this to true to make a default formatter disabled initially */
  $formatter->api_version = 2;
  $formatter->name = 'type_and_link';
  $formatter->label = 'File type, size and link';
  $formatter->description = '';
  $formatter->mode = 'php';
  $formatter->field_types = 'file';
  $formatter->code = '    $output = \'\';
    $langcode = $variables[\'#langcode\'];
    foreach ($variables[\'#items\'] as $item) {
       // reassign title
       $item[\'filename\'] =   $item[\'field_file_title\'][$langcode][0][\'value\'];

       $output .= \'<div class="file-item">\';
       $output .= theme(\'file_link\', array(\'file\' => (object) $item));

       $size = $item[\'filesize\'];
       if ($size > 0) {
         $units = array("B", "KB", "MB", "GB", "TB" );
         $digitGroups = (integer) (log10($size)/log10(1024));
         $output .=  \'<span class="filesize">(\'. number_format(($size/pow(1024, $digitGroups)), 2, \'.\', \'\') . \' \'.  $units[$digitGroups] .\')</span>\';
      }
      if (isset($item[\'field_file_description\']) and $summary = $item[\'field_file_description\'][$langcode][0][\'value\'] ) {
        $output .= \'<div class="file-summary">\'.  $summary .\'</div>\';
      }
      $output .= \'</div>\';
    }
    return $output;';
  $formatter->fapi = '';
  $formatters['type_and_link'] = $formatter;

  return $formatters;
}


